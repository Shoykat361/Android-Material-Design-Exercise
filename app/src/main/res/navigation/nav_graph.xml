<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/dashBoardFragment">
    <fragment
        android:id="@+id/introUidesignFragment"
        android:name="com.shoykat.androidmaterialdesign.IntroUidesignFragment"
        android:label="fragment_intro_uidesign"
        tools:layout="@layout/fragment_intro_uidesign" >
        <action
            android:id="@+id/action_introUidesignFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.shoykat.androidmaterialdesign.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.shoykat.androidmaterialdesign.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" />
    <fragment
        android:id="@+id/dashBoardFragment"
        android:name="com.shoykat.androidmaterialdesign.DashBoardFragment"
        android:label="fragment_dash_board"
        tools:layout="@layout/fragment_dash_board" />
</navigation>